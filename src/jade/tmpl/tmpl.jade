block config
doctype html
html
	head
		meta(charset='utf-8')
		meta(http-equiv="X-UA-Compatible" content="IE=edge,chrome=1")
		link(rel="icon" type="image/png" href="static/img/favicon.ico")
		if title
			title #{title}-易BANK-专业的供应链融资平台
		meta(name="keywords" content="#{title.split('-')}")
		meta(name="description" content="易BANK,专业的供应链融资平台")
		link(href="#{static}/css/base.css?=#{version}" rel="stylesheet")
		if cssname
			link(href="#{static}/css/#{cssname}.css?=#{version}" rel="stylesheet")
		block head
	body
		block header
		.p-toper
			dl.p-w.content
				dt.left 客服电话:4006-405-258
				dd.right
					em.uname
						a.cRed(href="") TOM
						|，您好
					em.msg
						a(href="") 消息
							span 10
					em.help
						a(href="") 帮助中心
					em.app
						a(href="") 手机客户端
		.p-header.p-w
			.logo.left
				a(href="")
					img(src="static/img/logo.png")
			dl.login.right
